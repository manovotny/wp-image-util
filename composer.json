{
    "name": "manovotny/wp-image-util",
    "version": "1.2.0",
    "description": "A collection of helpful utilities for working with images in WordPress.",
    "type": "library",
    "homepage": "https://github.com/manovotny/wp-image-util",
    "authors": [
        {
            "name": "Michael Novotny",
            "email": "manovotny@gmail.com",
            "homepage": "http://manovotny.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0+",
    "keywords": [
        "image",
        "utility",
        "wordpress"
    ],
    "require": {
        "manovotny/wp-dom-util": "~1.0",
        "manovotny/wp-url-util": "~2.0"
    },
    "require-dev": {
        "manovotny/wp-phpunit-helpers": "~1.0",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "files": [
            "wp-image-util.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "vendor/phpunit",
            "vendor/sebastian",
            "vendor/symfony"
        ]
    }
}
